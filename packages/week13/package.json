{
	"name": "@app-block/week13",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"compile": "hardhat compile",
		"build": "npm run compile && npm run build:types",
		"build:types": "HARDHAT_WEB3_TYPE=1 hardhat typechain",
		"build:test-types": "hardhat typechain",
		"deploy": "hardhat run",
		"verify": "hardhat verify --network goerli",
		"test": "npm run test:fork && npm run test:flashloan && npm run test:compound",
		"test:fork": "HARDHAT_FORK_TEST=1 hardhat test test/TestFork.spec.ts",
		"test:flashloan": "HARDHAT_FORK_TEST=1 hardhat test test/DynamicFlashLoan.spec.ts",
		"test:compound": "hardhat test ./test/compound/*.spec.ts",
		"blocknode": "hardhat node",
		"watch": "remixd -s . --remix-ide https://remix.ethereum.org",
		"clean": "rm -rf .deps && rm -rf artifacts && rm -rf cache && rm -rf web3-types && rm -rf test-types",
		"selector": "ts-node node-selector.ts"
	},
	"devDependencies": {
		"@ethersproject/abi": "^5.7.0",
		"@ethersproject/providers": "^5.7.0",
		"@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
		"@nomicfoundation/hardhat-network-helpers": "^1.0.4",
		"@nomicfoundation/hardhat-toolbox": "^1.0.2",
		"@nomiclabs/hardhat-ethers": "^2.1.1",
		"@nomiclabs/hardhat-etherscan": "^3.1.0",
		"@remix-project/remixd": "^0.6.5",
		"@typechain/ethers-v5": "^10.1.0",
		"@typechain/hardhat": "^6.1.2",
		"@typechain/web3-v1": "^6.0.1",
		"@types/chai": "^4.3.3",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.7.13",
		"chai": "^4.3.6",
		"dotenv": "^16.0.1",
		"ethers": "^5.7.0",
		"hardhat": "^2.10.2",
		"hardhat-gas-reporter": "^1.0.8",
		"prettier-plugin-solidity": "^1.0.0-beta.24",
		"solidity-coverage": "^0.7.21",
		"ts-node": "^10.9.1",
		"typechain": "^8.1.0",
		"typescript": "^4.8.2"
	},
	"dependencies": {
		"@aave/protocol-v2": "^1.0.1",
		"@app-block/week11": "workspace:^",
		"@openzeppelin/contracts": "^4.7.3",
		"@uniswap/smart-order-router": "^2.10.1",
		"@uniswap/v3-periphery": "^1.4.3",
		"compound-protocol": "https://github.com/compound-finance/compound-protocol.git",
		"web3": "^1.8.0"
	}
}
